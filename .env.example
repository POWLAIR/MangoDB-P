# MongoDB Atlas Configuration
MONGODB_URI=mongodb+srv://<USERNAME>:<PASSWORD>@<CLUSTER_URL>/?retryWrites=true&w=majority
MONGODB_DB_NAME=myDatabase

# Local MongoDB Configuration (Docker)
MONGODB_LOCAL_URI=mongodb://admin:password@mongodb:27017
MONGODB_LOCAL_DB_NAME=myDatabase

# Environment
NODE_ENV=development

# Note: Replace <USERNAME>, <PASSWORD>, and <CLUSTER_URL> with your MongoDB Atlas credentials 